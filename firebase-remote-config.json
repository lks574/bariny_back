{
  "conditions": [
    {
      "name": "iOS_users",
      "expression": "app.id == 'your.app.bundle.id' && device.os == 'ios'"
    },
    {
      "name": "beta_users",
      "expression": "user.inRandomPercentile(10)"
    },
    {
      "name": "development_mode",
      "expression": "app.id == 'your.app.bundle.id.dev'"
    }
  ],
  "parameters": {
    "quiz_version": {
      "defaultValue": {
        "value": "1.0.0"
      },
      "conditionalValues": {
        "development_mode": {
          "value": "1.0.0-dev"
        }
      },
      "description": "현재 퀴즈 데이터 버전"
    },
    "download_url": {
      "defaultValue": {
        "value": "https://your-project.supabase.co/storage/v1/object/public/quiz-files/quiz_data_v1.0.0.json"
      },
      "conditionalValues": {
        "development_mode": {
          "value": "https://your-project.supabase.co/storage/v1/object/public/quiz-files/quiz_data_v1.0.0-dev.json"
        }
      },
      "description": "퀴즈 데이터 다운로드 URL"
    },
    "categories": {
      "defaultValue": {
        "value": "person,general,country,drama,music"
      },
      "description": "사용 가능한 퀴즈 카테고리 목록"
    },
    "force_update": {
      "defaultValue": {
        "value": "false"
      },
      "conditionalValues": {
        "beta_users": {
          "value": "true"
        }
      },
      "description": "강제 업데이트 여부"
    },
    "maintenance_mode": {
      "defaultValue": {
        "value": "false"
      },
      "description": "점검 모드 여부"
    },
    "min_app_version": {
      "defaultValue": {
        "value": "1.0.0"
      },
      "description": "최소 요구 앱 버전"
    },
    "feature_flags": {
      "defaultValue": {
        "value": "ai_quiz:true,voice_mode:true,offline_mode:true"
      },
      "conditionalValues": {
        "beta_users": {
          "value": "ai_quiz:true,voice_mode:true,offline_mode:true,beta_features:true"
        }
      },
      "description": "기능 토글 플래그"
    },
    "auth_methods_enabled": {
      "defaultValue": {
        "value": "email,google,apple"
      },
      "description": "허용된 로그인 방식"
    },
    "social_login_required": {
      "defaultValue": {
        "value": "false"
      },
      "description": "소셜 로그인 강제 여부"
    },
    "guest_mode_enabled": {
      "defaultValue": {
        "value": "true"
      },
      "description": "게스트 모드 허용 여부"
    },
    "password_min_length": {
      "defaultValue": {
        "value": "8"
      },
      "description": "최소 비밀번호 길이"
    },
    "session_timeout_minutes": {
      "defaultValue": {
        "value": "60"
      },
      "description": "세션 타임아웃 (분)"
    },
    "max_login_attempts": {
      "defaultValue": {
        "value": "5"
      },
      "description": "최대 로그인 시도 횟수"
    },
    "auto_sync_enabled": {
      "defaultValue": {
        "value": "true"
      },
      "description": "자동 동기화 허용 여부"
    },
    "offline_mode_enabled": {
      "defaultValue": {
        "value": "true"
      },
      "description": "오프라인 모드 허용 여부"
    },
    "min_app_version_for_auth": {
      "defaultValue": {
        "value": "1.0.0"
      },
      "description": "인증 최소 앱 버전"
    },
    "deprecated_auth_notice": {
      "defaultValue": {
        "value": "앱을 최신 버전으로 업데이트해 주세요."
      },
      "description": "구버전 인증 경고 메시지"
    },
    "api_rate_limit": {
      "defaultValue": {
        "value": "100"
      },
      "description": "API 요청 제한 (시간당)"
    },
    "cache_duration_minutes": {
      "defaultValue": {
        "value": "60"
      },
      "description": "캐시 유지 시간 (분)"
    }
  },
  "version": {
    "versionNumber": "1",
    "updateTime": "2024-01-15T10:00:00Z",
    "updateUser": {
      "email": "admin@brainy-app.com"
    },
    "updateOrigin": "CONSOLE",
    "updateType": "INCREMENTAL_UPDATE"
  }
} 